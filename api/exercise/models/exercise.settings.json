{
  "kind": "collectionType",
  "collectionName": "exercises",
  "info": {
    "name": "exercise"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "richtext",
      "required": true
    },
    "time": {
      "type": "decimal"
    },
    "sets": {
      "type": "integer"
    },
    "reps": {
      "type": "integer"
    },
    "maxWeight": {
      "type": "decimal"
    },
    "completed": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "images": {
      "type": "json"
    }
  }
}
